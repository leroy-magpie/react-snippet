{
  "引入": {
    "prefix": "im",
    "body": ["import ${1:React} from '${2:react}';"]
  },
  "注释": {
    "prefix": "zs",
    "body": ["/** $1 */"]
  },
  "解构对象": {
    "prefix": "go",
    "body": ["const { $2 } = $1;"]
  },
  "方法": {
    "prefix": "ch",
    "body": ["const handle${1} = ($2) => {", "  ${3}", "};", ""]
  },
  "私有方法": {
    "prefix": "ph",
    "body": ["${1:handle} = ($2) => {", "  ${3}", "}", ""]
  },
  "index导出组件": {
    "prefix": "ext",
    "body": [
      "import ${1:temp} from './${1:temp}';",
      "",
      "export * from './${1:temp}';",
      "",
      "export default ${1:temp};"
    ],
    "description": ""
  },
  "connect包裹": {
    "prefix": "imc",
    "body": [
      "import { connect } from 'react-redux';",
      "",
      "import ${1:temp} from './${1:temp}';",
      "",
      "const mapState2Props = state => ({",
      "",
      "});",
      "",
      "const mapDispatch2Prop = {",
      "",
      "};",
      "",
      "export default connect(mapState2Props, mapDispatch2Prop)(${1:temp});"
    ]
  },
  "index导出connector组件": {
    "prefix": "exc",
    "body": [
      "import ${1:temp} from './connector';",
      "",
      "export default ${1:temp};"
    ],
    "description": ""
  }
}
