      "import './${1:temp}.less';",
      "",
      "export interface IProps {",
      "  emp?: string;",
      "}",
      "",
      "const ${1:temp}: React.FC<IProps> = memo(() => {",
      "  return (",
      "    <div>a</div>",
      "  );",
      "})",
      "",
      "${1:temp}.displayName = '${1:temp}'",
      "",
      "export default ${1:temp};",
      ""
    ]
  },
