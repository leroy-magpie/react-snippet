      "  emp?: string;",
      "}",
      "",
      "type IProps = IPropsExternal & IPropsConnected;",
      "",
      "const mapStateToProps = state => ({",
      "",
      "});",
      "",
      "const mapDispatchToProps = {",
      "",
      "};",
      "",
      "@connect(mapStateToProps, mapDispatchToProps)",
      "class ${1:temp} extends PureComponent<IProps> {",
      "  render() {",
      "    return (",
      "      <div>${2:a}</div>",
      "    );",
      "  }",
      "}",
      "",
